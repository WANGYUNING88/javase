package com.wang.math;

import java.util.Scanner;

public class Main2 {
	    public static void main(String[] args) {
	        Scanner sc = new Scanner(System.in);
	        int n = sc.nextInt();

	        int[] num1 = new int[n] ;
	        int i = 0;
	        while(i<n){
	        	
	        	
	        	num1[i] = sc.nextInt();
        		i++;
            
	        }
	        int k = sc.nextInt();
	        int[] num2 =new int[k];
	        int[] num3 =new int[k];
	        int j=0;
	        while(j<n){
	        	
	        	
	        	num2[i] = sc.nextInt();
        		j++;
            
	        }
	        for(int m = 0;m<k;m++){
	        	int sum = 0;
		        for(int o = 0;o<n;o++){	
		        	sum += num1[o];
		        	if(num2[m]>sum){
		        		System.out.println(n);
		        	}
		        }
	        }
	       
	    
	    }
	

}
