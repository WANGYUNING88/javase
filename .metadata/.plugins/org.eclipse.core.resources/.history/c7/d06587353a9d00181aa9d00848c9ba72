package com.wang.math;

import java.util.Scanner;

public class Main1 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int k = sc.nextInt();
        int x = 0;
        int[] num1 = null ,num2 =null;
        for(int i = 0; i < n; i++){
        	
        		x	= sc.nextInt();
        		num1[1] = x;
            
        } 
        for(int i = 0; i < n; i++){
        	num2[i] =  sc.nextInt();
            
        } 
        int sum = 0;
        for(int i = 0; i < n; i++){
           sum = num1[i]*num2[i];
        } 
        System.out.println(sum);
    }
}