package com.wang.example;

public class Math {
	
	public static String isSanjiaoxing(float a,float b,float c){
		
		String result = "²»ÊÇ";
		
		if(a!=0||b!=0||c!=0){
			
			if(a+b>c&&a+c>b&&b+c>a){
				result = "ÊÇ";
				return result;
						
			}
			
			return result;
		}
		
		return result;
		
	}

}
